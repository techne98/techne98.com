---
const { pageTitle } = Astro.props;
---

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width initial-scale=1.0" />
    <meta name="generator" content={Astro.generator} />
    <meta property="og:image" content="https://techne98.com/og.png" />
    <meta property="og:title" content="techne98" />
    <meta
        property="og:description"
        content="Thoughts, guides, and learnings on programming and systems"
    />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="techne98.com" />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="630" />
    <title>{pageTitle} - techne98.com</title>
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96" />
    <link rel="icon" href="/favicon.ico" />
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <link rel="sitemap" href="/sitemap-index.xml" />
    <link
        rel="alternate"
        type="application/rss+xml"
        title="techne98"
        href={new URL("rss.xml", Astro.site)}
    />
</head>
